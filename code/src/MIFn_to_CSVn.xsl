<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
<!-- XSLT stylesheet to convert canonical PSI-MI files into a CSV file,-->
<!-- more convenient for bilding tables. The separator used in the     -->
<!-- output file is ";". The file is divided into 5 paragraphs :       -->
<!-- Availability List, Experiment List, Interactor List,              -->
<!-- Interaction List and Feature List.                                -->
<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
<!-- This file is based on the XSD schema version (CVS) 1.12.          -->
<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
<!-- Written by Cezanne, Even, Roumegous, Jolibert, Thomas-Nelson,     -->
<!-- Marques, Cros, Sablayrolles at the ENSEIRB (www.enseirb.fr)       -->
<!-- with a little advice from David Sherman 2003/05/20                -->
<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->

<xsl:stylesheet version="1.0"
  xmlns:psi="net:sf:psidev:mi" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:xsl="http://www.w3.org/1999/XSL/Transform">

<xsl:output method="text"/>

<xsl:namespace-alias
     stylesheet-prefix = "xsi"
     result-prefix="psi"/>

<!-- Take care of not introducing extra spaces, tabulations or returns !! -->
<xsl:template match="psi:entrySet"><xsl:apply-templates select="psi:entry"/></xsl:template>

<!-- The output file is divided into 5 paragraphs -->
<xsl:template match="psi:entry">
[_Availability List_];
<xsl:apply-templates select="psi:availabilityList"/>
    
[_Experiment List_];
<xsl:apply-templates select="psi:experimentList"/>
    
[_Interactor List_];
<xsl:apply-templates select="psi:interactorList"/>
    
[_Interaction List_];
<!-- In this paragraph each ligne fits in with a participant of an interaction with a reference to it. -->
<xsl:apply-templates select="psi:interactionList" mode="interaction_table"/>
  
[_Feature List_];
<!-- This paragraph lists all the features defined in the file with a reference to the interaction. -->
<xsl:apply-templates select="psi:interactionList" mode="feature_table"/>
</xsl:template>


<!-- 1st paragraph : availabilityList -->
<xsl:template match="psi:availabilityList">[availability id];[availability];
<!-- Each loop correspond to one line. --><xsl:for-each select="psi:availability"><xsl:value-of select="current()/@id"/>;<xsl:value-of select="current()"/>;
</xsl:for-each></xsl:template>
  
<!-- 2nd paragraph : experimentList -->
<xsl:template match="psi:experimentList">[id];[shortLabel];[fullName];[bibref primaryRef db];[bibref primaryRef id];[bibref primaryRef secondary];[bibref primaryRef version];[bibref secondaryRef db];[bibref secondaryRef id];[bibref secondaryRef secondary];[bibref secondaryRef version];[primaryRef db];[primaryRef id];[primaryRef secondary];[primaryRef version];[secondaryRef db];[secondaryRef id];[secondaryRef secondary];[secondaryRef version];[hostOrganism ncbiTaxId];[hostOrganism shortLabel];[hostOrganism fullName];[hostOrganism cellType shortLabel];[hostOrganism cellType fullName];[hostOrganism cellType primaryRef db];[hostOrganism cellType primaryRef id];[hostOrganism cellType primaryRef secondary];[hostOrganism cellType primaryRef version];[hostOrganism cellType secondaryRef db];[hostOrganism cellType secondaryRef id];[hostOrganism cellType secondaryRef secondary];[hostOrganism cellType secondaryRef version];[hostOrganism compartment shortLabel];[hostOrganism compartment fullName];[hostOrganism compartment primaryRef db];[hostOrganism compartment primaryRef id];[hostOrganism compartment primaryRef secondary];[hostOrganism compartment primaryRef version];[hostOrganism compartiment secondaryRef db];[hostOrganism compartment secondaryRef id];[hostOrganism compartment secondaryRef secondary];[hostOrganism compartiment secondaryRef version];[hostOrganism tissue shortLabel];[hostOrganism tissue fullName];[hostOrganism tissue primaryRef db];[hostOrganism tissue primaryRef id];[hostOrganism tissue primaryRef secondary];[hostOrganism tissue primaryRef version];[hostOrganism tissue secondaryRef db];[hostOrganism tissue secondaryRef id];[hostOrganism tissue secondaryRef secondary];[hostOrganism tissue secondaryRef version];[interactionDetection shortLabel];[interactionDetection fullname];[interactionDetection primaryRef db];[interactionDetection primaryRef id];[interactionDetection primaryRef secondary];[interactionDetection primaryRef version];[interactionDetection secondaryRef db];[interactionDetection secondaryRef id];[interactionDetection secondaryRef secondary];[interactionDetection secondaryRef version];[participantDetection shortLabel];[participantDetection fullname];[participantDetection primaryRef db];[participantDetection primaryRef id];[participantDetection primaryRef secondary];[participantDetection primaryRef version];[participantDetection secondaryRef db];[participantDetection secondaryRef id];[participantDetection secondaryRef secondary];[participantDetection secondaryRef version];[featureDetection shortLabel];[featureDetection fullname];[featureDetection primaryRef db];[featureDetection primaryRef id];[featureDetection primaryRef secondary];[featureDetection primaryRef version];[featureDetection secondaryRef db];[featureDetection secondaryRef id];[featureDetection secondaryRef secondary];[featureDetection secondaryRef version];[confidence unit];[confidence value];
<xsl:for-each select="psi:experimentDescription"><xsl:value-of select="current()/@id"/>;<xsl:value-of select="psi:names/psi:shortLabel"/>;<xsl:value-of select="psi:names/psi:fullName"/>;<xsl:value-of select="psi:bibref/psi:xref/psi:primaryRef/@db"/>;<xsl:value-of select="psi:bibref/psi:xref/psi:primaryRef/@id"/>;<xsl:value-of select="psi:bibref/psi:xref/psi:primaryRef/@secondary"/>;<xsl:value-of select="psi:bibref/psi:xref/psi:primaryRef/@version"/>;<xsl:value-of select="psi:bibref/psi:xref/psi:secondaryRef/@db"/>;<xsl:value-of select="psi:bibref/psi:xref/psi:secondaryRef/@id"/>;<xsl:value-of select="psi:bibref/psi:xref/psi:secondaryRef/@secondary"/>;<xsl:value-of select="psi:bibref/psi:xref/psi:secondaryRef/@version"/>;<xsl:value-of select="psi:xref/psi:primaryRef/@db"/>;<xsl:value-of select="psi:xref/psi:primaryRef/@id"/>;<xsl:value-of select="psi:xref/psi:primaryRef/@secondary"/>;<xsl:value-of select="psi:xref/psi:primaryRef/@version"/>;<xsl:value-of select="psi:xref/psi:secondaryRef/@db"/>;<xsl:value-of select="psi:xref/psi:secondaryRef/@id"/>;<xsl:value-of select="psi:xref/psi:secondaryRef/@secondary"/>;<xsl:value-of select="psi:xref/psi:secondaryRef/@version"/>;<xsl:value-of select="psi:hostOrganism/@ncbiTaxId"/>;<xsl:value-of select="psi:hostOrganism/psi:names/psi:shortLabel"/>;<xsl:value-of select="psi:hostOrganism/psi:names/psi:fullName"/>;<xsl:value-of select="psi:hostOrganism/psi:cellType/psi:names/psi:shortLabel"/>;<xsl:value-of select="psi:hostOrganism/psi:cellType/psi:names/psi:fullName"/>;<xsl:value-of select="psi:hostOrganism/psi:cellType/psi:xref/psi:primaryRef/@db"/>;<xsl:value-of select="psi:hostOrganism/psi:cellType/psi:xref/psi:primaryRef/@id"/>;<xsl:value-of select="psi:hostOrganism/psi:cellType/psi:xref/psi:primaryRef/@secondary"/>;<xsl:value-of select="psi:hostOrganism/psi:cellType/psi:xref/psi:primaryRef/@version"/>;<xsl:value-of select="psi:hostOrganism/psi:cellType/psi:xref/psi:secondaryRef/@db"/>;<xsl:value-of select="psi:hostOrganism/psi:cellType/psi:xref/psi:secondaryRef/@id"/>;<xsl:value-of select="psi:hostOrganism/psi:cellType/psi:xref/psi:secondaryRef/@secondary"/>;<xsl:value-of select="psi:hostOrganism/psi:cellType/psi:xref/psi:secondaryRef/@version"/>;<xsl:value-of select="psi:hostOrganism/psi:compartment/psi:names/psi:shortLabel"/>;<xsl:value-of select="psi:hostOrganism/psi:compartment/psi:names/psi:fullName"/>;<xsl:value-of select="psi:hostOrganism/psi:compartment/psi:xref/psi:primaryRef/@db"/>;<xsl:value-of select="psi:hostOrganism/psi:compartment/psi:xref/psi:primaryRef/@id"/>;<xsl:value-of select="psi:hostOrganism/psi:compartment/psi:xref/psi:primaryRef/@secondary"/>;<xsl:value-of select="psi:hostOrganism/psi:compartment/psi:xref/psi:primaryRef/@version"/>;<xsl:value-of select="psi:hostOrganism/psi:compartment/psi:xref/psi:secondaryRef/@db"/>;<xsl:value-of select="psi:hostOrganism/psi:compartment/psi:xref/psi:secondaryRef/@id"/>;<xsl:value-of select="psi:hostOrganism/psi:compartment/psi:xref/psi:secondaryRef/@secondary"/>;<xsl:value-of select="psi:hostOrganism/psi:compartment/psi:xref/psi:secondaryRef/@version"/>;<xsl:value-of select="psi:hostOrganism/psi:tissue/psi:names/psi:shortLabel"/>;<xsl:value-of select="psi:hostOrganism/psi:tissue/psi:names/psi:fullName"/>;<xsl:value-of select="psi:hostOrganism/psi:tissue/psi:xref/psi:primaryRef/@db"/>;<xsl:value-of select="psi:hostOrganism/psi:tissue/psi:xref/psi:primaryRef/@id"/>;<xsl:value-of select="psi:hostOrganism/psi:tissue/psi:xref/psi:primaryRef/@secondary"/>;<xsl:value-of select="psi:hostOrganism/psi:tissue/psi:xref/psi:primaryRef/@version"/>;<xsl:value-of select="psi:hostOrganism/psi:tissue/psi:xref/psi:secondaryRef/@db"/>;<xsl:value-of select="psi:hostOrganism/psi:tissue/psi:xref/psi:secondaryRef/@id"/>;<xsl:value-of select="psi:hostOrganism/psi:tissue/psi:xref/psi:secondaryRef/@secondary"/>;<xsl:value-of select="psi:hostOrganism/psi:tissue/psi:xref/psi:secondaryRef/@version"/>;<xsl:value-of select="psi:interactionDetection/psi:names/psi:shortLabel"/>;<xsl:value-of select="psi:interactionDetection/psi:names/psi:fullName"/>;<xsl:value-of select="psi:interactionDetection/psi:xref/psi:primaryRef/@db"/>;<xsl:value-of select="psi:interactionDetection/psi:xref/psi:primaryRef/@id"/>;<xsl:value-of select="psi:interactionDetection/psi:xref/psi:primaryRef/@secondary"/>;<xsl:value-of select="psi:interactionDetection/psi:xref/psi:primaryRef/@version"/>;<xsl:value-of select="psi:interactionDetection/psi:xref/psi:secondaryRef/@db"/>;<xsl:value-of select="psi:interactionDetection/psi:xref/psi:secondaryRef/@id"/>;<xsl:value-of select="psi:interactionDetection/psi:xref/psi:secondaryRef/@secondary"/>;<xsl:value-of select="psi:interactionDetection/psi:xref/psi:secondaryRef/@version"/>;<xsl:value-of select="psi:participantDetection/psi:names/psi:shortLabel"/>;<xsl:value-of select="psi:participantDetection/psi:names/psi:fullName"/>;<xsl:value-of select="psi:participantDetection/psi:xref/psi:primaryRef/@db"/>;<xsl:value-of select="psi:participantDetection/psi:xref/psi:primaryRef/@id"/>;<xsl:value-of select="psi:participantDetection/psi:xref/psi:primaryRef/@secondary"/>;<xsl:value-of select="psi:participantDetection/psi:xref/psi:primaryRef/@version"/>;<xsl:value-of select="psi:participantDetection/psi:xref/psi:secondaryRef/@db"/>;<xsl:value-of select="psi:participantDetection/psi:xref/psi:secondaryRef/@id"/>;<xsl:value-of select="psi:participantDetection/psi:xref/psi:secondaryRef/@secondary"/>;<xsl:value-of select="psi:participantDetection/psi:xref/psi:secondaryRef/@version"/>;<xsl:value-of select="psi:featureDetection/psi:names/psi:shortLabel"/>;<xsl:value-of select="psi:featureDetection/psi:names/psi:fullName"/>;<xsl:value-of select="psi:featureDetection/psi:xref/psi:primaryRef/@db"/>;<xsl:value-of select="psi:featureDetection/psi:xref/psi:primaryRef/@id"/>;<xsl:value-of select="psi:featureDetection/psi:xref/psi:primaryRef/@secondary"/>;<xsl:value-of select="psi:featureDetection/psi:xref/psi:primaryRef/@version"/>;<xsl:value-of select="psi:featureDetection/psi:xref/psi:secondaryRef/@db"/>;<xsl:value-of select="psi:featureDetection/psi:xref/psi:secondaryRef/@id"/>;<xsl:value-of select="psi:featureDetection/psi:xref/psi:secondaryRef/@secondary"/>;<xsl:value-of select="psi:featureDetection/psi:xref/psi:secondaryRef/@version"/>;<xsl:value-of select="psi:confidence/@unit"/>;<xsl:value-of select="psi:confidence/@value"/>;
</xsl:for-each></xsl:template>

<!-- 3d paragraph : interactorList -->
<xsl:template match="psi:interactorList">[id];[shortLabel];[fullName];[primaryRef db];[primaryRef id];[primaryRef secondary];[primaryRef version];[secondaryRef db];[secondaryRef id];[secondaryRef secondary];[secondaryRef version];[organism ncbiTaxId];[organism shortLabel];[organism fullName];[organism cellType shortLabel];[organism cellType fullName];[organism cellType primaryRef db];[organism cellType primaryRef id];[organism cellType primaryRef secondary];[organism cellType primaryRef version];[organism cellType secondaryRef db];[organism cellType secondaryRef id];[organism cellType secondaryRef secondary];[organism cellType secondaryRef version];[organism compartment shortLabel];[organism compartment fullName];[organism compartment primaryRef db];[organism compartment primaryRef id];[organism compartment primaryRef secondary];[organism compartment primaryRef version];[organism compartiment secondaryRef db];[organism compartment secondaryRef id];[organism compartment secondaryRef secondary];[organism compartiment secondaryRef version];[organism tissue shortLabel];[organism tissue fullName];[organism tissue primaryRef db];[organism tissue primaryRef id];[organism tissue primaryRef secondary];[organism tissue primaryRef version];[organism tissue secondaryRef db];[organism tissue secondaryRef id];[organism tissue secondaryRef secondary];[organism tissue secondaryRef version];[protein sequence];
<xsl:for-each select="psi:proteinInteractor"><xsl:value-of select="current()/@id"/>;<xsl:value-of select="psi:names/psi:shortLabel"/>;<xsl:value-of select="psi:names/psi:fullName"/>;<xsl:value-of select="psi:xref/psi:PrimaryRef/@db"/>;<xsl:value-of select="psi:xref/psi:PrimaryRef/@id"/>;<xsl:value-of select="psi:xref/psi:PrimaryRef/@secondary"/>;<xsl:value-of select="psi:xref/psi:PrimaryRef/@version"/>;<xsl:value-of select="psi:xref/psi:SecondaryRef/@db"/>;<xsl:value-of select="psi:xref/psi:SecondaryRef/@id"/>;<xsl:value-of select="psi:xref/psi:SecondaryRef/@secondary"/>;<xsl:value-of select="psi:xref/psi:SecondaryRef/@version"/>;<xsl:value-of select="psi:organism/@ncbiTaxId"/>;<xsl:value-of select="psi:organism/psi:names/psi:shortLabel"/>;<xsl:value-of select="psi:organism/psi:names/psi:fullName"/>;<xsl:value-of select="psi:organism/psi:cellType/psi:names/psi:shortLabel"/>;<xsl:value-of select="psi:organism/psi:cellType/psi:names/psi:fullName"/>;<xsl:value-of select="psi:organism/psi:cellType/psi:xref/psi:primaryRef/@db"/>;<xsl:value-of select="psi:organism/psi:cellType/psi:xref/psi:primaryRef/@id"/>;<xsl:value-of select="psi:organism/psi:cellType/psi:xref/psi:primaryRef/@secondary"/>;<xsl:value-of select="psi:organism/psi:cellType/psi:xref/psi:primaryRef/@version"/>;<xsl:value-of select="psi:organism/psi:cellType/psi:xref/psi:secondaryRef/@db"/>;<xsl:value-of select="psi:organism/psi:cellType/psi:xref/psi:secondaryRef/@id"/>;<xsl:value-of select="psi:organism/psi:cellType/psi:xref/psi:secondaryRef/@secondary"/>;<xsl:value-of select="psi:organism/psi:cellType/psi:xref/psi:secondaryRef/@version"/>;<xsl:value-of select="psi:organism/psi:compartment/psi:names/psi:shortLabel"/>;<xsl:value-of select="psi:organism/psi:compartment/psi:names/psi:fullName"/>;<xsl:value-of select="psi:organism/psi:compartment/psi:xref/psi:primaryRef/@db"/>;<xsl:value-of select="psi:organism/psi:compartment/psi:xref/psi:primaryRef/@id"/>;<xsl:value-of select="psi:organism/psi:compartment/psi:xref/psi:primaryRef/@secondary"/>;<xsl:value-of select="psi:organism/psi:compartment/psi:xref/psi:primaryRef/@version"/>;<xsl:value-of select="psi:organism/psi:compartment/psi:xref/psi:secondaryRef/@db"/>;<xsl:value-of select="psi:organism/psi:compartment/psi:xref/psi:secondaryRef/@id"/>;<xsl:value-of select="psi:organism/psi:compartment/psi:xref/psi:secondaryRef/@secondary"/>;<xsl:value-of select="psi:organism/psi:compartment/psi:xref/psi:secondaryRef/@version"/>;<xsl:value-of select="psi:organism/psi:tissue/psi:names/psi:shortLabel"/>;<xsl:value-of select="psi:organism/psi:tissue/psi:names/psi:fullName"/>;<xsl:value-of select="psi:organism/psi:tissue/psi:xref/psi:primaryRef/@db"/>;<xsl:value-of select="psi:organism/psi:tissue/psi:xref/psi:primaryRef/@id"/>;<xsl:value-of select="psi:organism/psi:tissue/psi:xref/psi:primaryRef/@secondary"/>;<xsl:value-of select="psi:organism/psi:tissue/psi:xref/psi:primaryRef/@version"/>;<xsl:value-of select="psi:organism/psi:tissue/psi:xref/psi:secondaryRef/@db"/>;<xsl:value-of select="psi:organism/psi:tissue/psi:xref/psi:secondaryRef/@id"/>;<xsl:value-of select="psi:organism/psi:tissue/psi:xref/psi:secondaryRef/@secondary"/>;<xsl:value-of select="psi:organism/psi:tissue/psi:xref/psi:secondaryRef/@version"/>;<xsl:value-of select="psi:proteinseq/psi:sequence"/>;
</xsl:for-each></xsl:template>

<!-- 4th paragraph : interactionList -->
<xsl:template match="psi:interactionList" mode="interaction_table">[proteinParticipant InteractorRef];[interaction_number];[interaction shortLabel];[interaction fullName];[experimentRef];[availabilityRef];[proteinParticipantConfidence unit];[proteinParticipantConfidence value];[role];[isTaggedProtein];[isOverexpressed];[interactionType shortLabel];[interactionType fullName];[interactionType primaryRef db];[interactionType primaryRef id];[interactionType primaryRef secondary];[interactionType primaryRef version];[interactionType secondaryRef db];[interactionType secondaryRef id];[interactionType secondaryRef secondary];[interactionType secondaryRef version];[confidence unit];[confidence value];
<!-- ATTENTION : here each ligne fits in with a proteinParticipant --><xsl:for-each select="psi:interaction"><!-- we number each interaction with its position in the source file -->
<xsl:variable name = "int_number" ><xsl:value-of select="position()"/></xsl:variable>
<xsl:variable name = "int_names" ><xsl:value-of select="psi:names/psi:shortLabel"/>;<xsl:value-of select="psi:names/psi:fullName"/></xsl:variable>
<xsl:variable name = "int_exp" ><xsl:value-of select="psi:experimentRef/@ref"/></xsl:variable>
<xsl:variable name = "int_lis" ><xsl:value-of select="psi:availabilityRef/@ref"/></xsl:variable>
<xsl:variable name = "int_type_names" ><xsl:value-of select="psi:interactionType/psi:names/psi:shortLabel"/>;<xsl:value-of select="psi:interactionType/psi:names/psi:fullName"/></xsl:variable>
<xsl:variable name = "int_type_xref" ><xsl:value-of select="psi:interactionType/psi:xref/psi:primaryRef/@db"/>;<xsl:value-of select="psi:interactionType/psi:xref/psi:primaryRef/@id"/>;<xsl:value-of select="psi:interactionType/psi:xref/psi:primaryRef/@secondary"/>;<xsl:value-of select="psi:interactionType/psi:xref/psi:primaryRef/@version"/>;<xsl:value-of select="psi:interactionType/psi:xref/psi:secondaryRef/@db"/>;<xsl:value-of select="psi:interactionType/psi:xref/psi:secondaryRef/@id"/>;<xsl:value-of select="psi:interactionType/psi:xref/psi:secondaryRef/@secondary"/>;<xsl:value-of select="psi:interactionType/psi:xref/psi:secondaryRef/@version"/></xsl:variable>
<xsl:variable name = "int_confidence" ><xsl:value-of select="psi:confidence/@unit"/>;<xsl:value-of select="psi:confidence/@value"/></xsl:variable>

<xsl:for-each select="psi:participantList/psi:proteinParticipant"><!-- we number each proteinParticipant with his unique reference --><xsl:value-of select="psi:interactorRef/@ref"/>;<xsl:value-of select="$int_number"/>;<xsl:value-of select="$int_names"/>;<xsl:value-of select="$int_exp"/>;<xsl:value-of select="$int_lis"/>;<xsl:value-of select="psi:confidence/@unit"/>;<xsl:value-of select="psi:confidence/@value"/>;<xsl:value-of select="psi:role"/>;<xsl:value-of select="psi:isTaggedProtein"/>;<xsl:value-of select="psi:isOverexpressedProtein"/>;<xsl:value-of select="$int_type_names"/>;<xsl:value-of select="$int_type_xref"/>;<xsl:value-of select="$int_confidence"/>;
</xsl:for-each></xsl:for-each></xsl:template>

<!-- 5th paragraph : featureList -->
<xsl:template match="psi:interactionList" mode="feature_table">[interaction_number];[proteinParticipant InteractorRef];[feature primaryRef db];[feature primaryRef id];[feature primaryRef secondary];[feature primaryRef version];[feature secondaryRef db];[feature secondaryRef id];[feature secondaryRef secondary];[feature secondaryRef version];[featureDescription shortLabel];[featureDescription fullName];[featureDescription primaryRef db];[featureDescription primaryRef id];[featureDescription primaryRef secondary];[featureDescription primaryRef version];[featureDescription secondaryRef db];[featureDescription secondaryRef id];[featureDescription secondaryRef secondary];[featureDescription secondaryRef version];[location begin];[location end];[location beginInterval begin];[location beginInterval end];[location endInterval begin];[location endInterval end];[location];[location positionInterval begin];[location positionInterval end];[location site];[featureDetection shortLabel];[featureDetection fullName];[featureDetection primaryRef db];[featureDetection primaryRef id];[featureDetection primaryRef secondary];[featureDetection primaryRef version];[featureDetection secondaryRef db];[featureDetection secondaryRef id];[featureDetection secondaryRef secondary];[featureDetection secondaryRef version]; 
<xsl:for-each select="psi:interaction"><xsl:variable name = "int_number" ><xsl:value-of select="position()"/></xsl:variable>
<xsl:for-each select="psi:participantList/psi:proteinParticipant/psi:featureList/psi:feature"><!-- infos du feature --><xsl:value-of select="$int_number"/>;<xsl:value-of select="../../psi:interactorRef/@ref"/>;<xsl:value-of select="psi:xref/psi:primaryRef/@db"/>;<xsl:value-of select="psi:xref/psi:primaryRef/@id"/>;<xsl:value-of select="psi:xref/psi:primaryRef/@secondary"/>;<xsl:value-of select="psi:xref/psi:primaryRef/@version"/>;<xsl:value-of select="psi:xref/psi:secondaryRef/@db"/>;<xsl:value-of select="psi:xref/psi:secondaryRef/@id"/>;<xsl:value-of select="psi:xref/psi:secondaryRef/@secondary"/>;<xsl:value-of select="psi:xref/psi:secondaryRef/@version"/>;<xsl:value-of select="psi:featureDescription/psi:names/psi:shortLabel"/>;<xsl:value-of select="psi:featureDescription/psi:names/psi:fullName"/>;<xsl:value-of select="psi:featureDescription/psi:xref/psi:primaryRef/@db"/>;<xsl:value-of select="psi:featureDescription/psi:xref/psi:primaryRef/@id"/>;<xsl:value-of select="psi:featureDescription/psi:xref/psi:primaryRef/@secondary"/>;<xsl:value-of select="psi:featureDescription/psi:xref/psi:primaryRef/@version"/>;<xsl:value-of select="psi:featureDescription/psi:xref/psi:secondaryRef/@db"/>;<xsl:value-of select="psi:featureDescription/psi:xref/psi:secondaryRef/@id"/>;<xsl:value-of select="psi:featureDescription/psi:xref/psi:secondaryRef/@secondary"/>;<xsl:value-of select="psi:featureDescription/psi:xref/psi:secondaryRef/@version"/>;<xsl:value-of select="psi:location/psi:begin/@position"/>;<xsl:value-of select="psi:location/psi:end/@position"/>;<xsl:value-of select="psi:location/psi:beginInterval/@begin"/>;<xsl:value-of select="psi:location/psi:beginInterval/@end"/>;<xsl:value-of select="psi:location/psi:endInterval/@begin"/>;<xsl:value-of select="psi:location/psi:endInterval/@end"/>;<xsl:value-of select="psi:location/psi:position/@position"/>;<xsl:value-of select="psi:location/psi:positionInterval/@begin"/>;<xsl:value-of select="psi:location/psi:positionInterval/@end"/>;<xsl:value-of select="psi:location/psi:site/@position"/>;<xsl:value-of select="psi:featureDetection/psi:names/psi:shortLabel"/>;<xsl:value-of select="psi:featureDetection/psi:names/psi:fullName"/>;<xsl:value-of select="psi:featureDetection/psi:xref/psi:primaryRef/@db"/>;<xsl:value-of select="psi:featureDetection/psi:xref/psi:primaryRef/@id"/>;<xsl:value-of select="psi:featureDetection/psi:xref/psi:primaryRef/@secondary"/>;<xsl:value-of select="psi:featureDetection/psi:xref/psi:primaryRef/@version"/>;<xsl:value-of select="psi:featureDetection/psi:xref/psi:secondaryRef/@db"/>;<xsl:value-of select="psi:featureDetection/psi:xref/psi:secondaryRef/@id"/>;<xsl:value-of select="psi:featureDetection/psi:xref/psi:secondaryRef/@secondary"/>;<xsl:value-of select="psi:featureDetection/psi:xref/psi:secondaryRef/@version"/>;
</xsl:for-each></xsl:for-each></xsl:template>
</xsl:stylesheet>



